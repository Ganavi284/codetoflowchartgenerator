flowchart TD
  START(["start"])
  N1["var x, y: integer"]
  N2["x := 2"]
  N3["y := 0"]
  N4{"case x"}
  N5["case 1:"]
  N6[/"writeln('One')"/]
  N7["case 2:"]
  N8{"if y > 0"}
  N9[/"writeln('Positive')"/]
  N10[/"writeln('Not positive')"/]
  N11[/"writeln('Other')"/]
  END(["end"])
  START --> N1
  N1 --> N2
  N2 --> N3
  N3 --> N4
  N4 --> N5
  N5 --> N6
  N4 --> N7
  N7 --> N8
  N8 -- Yes --> N9
  N8 -- No --> N10
  N9 --> N11
  N10 --> N11
  N6 --> END
  N11 --> END