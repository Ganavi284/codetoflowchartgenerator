flowchart TD
  START(["start"])
  N1["var x, y: integer"]
  N2["x := 5"]
  N3["y := 0"]
  N4{"case x"}
  N5["case 1:"]
  N6[/"writeln('One')"/]
  N7["case 2:"]
  N8{"if y > 0"}
  N9[/"writeln('y is positive')"/]
  N10[/"writeln('y is not positive')"/]
  N11["case 3:"]
  N12[/"writeln('Three')"/]
  N13[/"writeln('Other number')"/]
  END(["end"])
  START --> N1
  N1 --> N2
  N2 --> N3
  N3 --> N4
  N4 --> N5
  N5 --> N6
  N4 --> N7
  N7 --> N8
  N8 -- Yes --> N9
  N8 -- No --> N10
  N4 --> N11
  N9 --> N12
  N10 --> N12
  N12 --> N13
  N6 --> END
  N12 --> END
  N13 --> END